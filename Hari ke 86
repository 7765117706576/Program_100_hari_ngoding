import java.util.Scanner;
public class Main {
    
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.print("Masukkan batas angka : ");
        int a = sc.nextInt();
        int b = (int) (Math.random() * a) + 1;
        int c = (int) (Math.random()*a)+ 1;
            
        System.out.println("Output bilangan acak : ");   
        System.out.println("Bilangan acak pertama : "+b);
        System.out.println("Bilangan acak kedua : "+c);    
    }
    
}
