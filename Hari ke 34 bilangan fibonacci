package fibonacci;
import java.util.Scanner;
public class Main {
    
    public static void main(String[] args) {
      Scanner input= new Scanner(System.in);
        int f1 = 0;
        int f2 = 1;
        System.out.print("Masukkan jumlah deretan yang bernilai: ");
        int n =input.nextInt();
        
        int fn;
        System.out.println("Deretan fibonacci");
        System.out.print(f1+", ");
        System.out.print(f2);
        for(int i= 1; i < n; i++){
            fn=f2+f1;
            System.out.print(", "+fn);
            f1= f2;
            f2= fn;
            
            
        }
        
    }
    
}
