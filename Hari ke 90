
public class Main {
    
    public static void main(String[] args) {
        String hewan[]={
            "Ayam",
            "Kambing",
            "Sapi",
            "Domba",
            "Ikan"
        };
        int x=0;
        int y=hewan.length-1;
        for(int i = 0 ; i < 3; i++){
            int angka= x+(int) (Math.random()*((y-x)+1));
            System.out.println(hewan[angka]);
        }
    }
    
}
